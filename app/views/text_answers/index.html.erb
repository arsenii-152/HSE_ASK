<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Answers</title>
</head>
<body>
<h1>Answers</h1>
<table>
  <thead>
  <tr>
    <th>result</th>
    <th>user_id</th>
    <th>question_id</th>
  </tr>
  </thead>

  <tbody>
  <% @text_answers.each do |text_answer| %>
    <tr>
      <td><%= text_answer.text_response %></td>
      <td><%= text_answer.user_id %></td>
      <td><%= text_answer.text_question_id %></td>
    </tr>
  <% end %>
  </tbody>
</table>
</body>
</html>
