<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Users</title>
</head>
<body>
<h1>Users</h1>
<table>
  <thead>
  <tr>
    <th>email</th>
    <th>name</th>
    <th>surname</th>
    <th>group</th>
    <th>isAdmin</th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.name %></td>
      <td><%= user.surname %></td>
      <td><%= user.group %></td>
      <td><%= user.admin %></td>
    </tr>
  <% end %>
  </tbody>
</table>
</body>
</html>